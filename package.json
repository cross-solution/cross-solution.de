    "hellojs": "^1.18.1",
    "iso-639-1": "^2.1.0",
    "jspdf": "^1.5.3",
    "prismjs": "^1.15.0",
    "quasar": "^1.17.2",
    "vue-cropperjs": "^4.0.1",
    "vue-currency-input": "^1.14.3",
    "vue-google-autocomplete": "^1.1.0",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.4.1",

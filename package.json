{
  "name": "cross-solution.de",
  "version": "0.1.0",
  "description": "CROSS Solution is a public first open company",
  "productName": "SmartComposer",
  "license": "MIT",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Carsten Bleek <bleek@cross-solution.de>",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "start": "node dist/ssr/index.js",
    "lintfix": "node --preserve-symlinks --preserve-symlinks-main ./node_modules/eslint/bin/eslint --ext .js,.vue src --fix",
    "test:jest": "quasar test --unit jest",
    "dev": "quasar dev",
    "dev:ssr": "quasar dev -m ssr",
    "build-linux": "quasar build -m electron linux -t mat -b builder",
    "build-win32": "quasar build -m electron win32 -t mat -b builder",
    "build-mac": "quasar build -m electron mac -t mat -b builder",
    "build": "./node_modules/.bin/quasar build -m ssr",
    "serve": "node dist/ssr/index.js"
  },
  "dependencies": {
    "@quasar/cli": "^1.2.2",
    "@quasar/extras": "^1.12.3",
    "@vuex-orm/core": "^0.34.1",
